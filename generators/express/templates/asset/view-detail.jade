extends layout

block content
  h1= title
  br
  table(class='table table-bordered')
    <% for(var x=0;x<currentAsset.properties.length;x++) { %>
    tr
      td
        b <%= currentAsset.properties[x].name %>
      td #{<%= currentAsset.name %>.<%= currentAsset.properties[x].name %>}
      <% } %>
  br
  a(href="/<%= currentAsset.name %>" class="btn btn-default" style="margin: 2px;") Back to list
